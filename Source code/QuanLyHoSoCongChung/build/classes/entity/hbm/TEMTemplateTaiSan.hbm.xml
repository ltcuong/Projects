<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">
	<class name="TEMTemplateTaiSan" table="tem_templatetaisan" lazy="true">	
		<id name="templateTaiSanID" column="TemplateTaiSanID"/>
			
		<property name="maTemplateTaiSan" column="MaTemplateTaiSan"/>
		<property name="tenTemplateTaiSan" column="TenTemplateTaiSan"/>		
		<property name="noiDungTemplateTaiSan" column="NoiDungTemplateTaiSan"/>	
		<property name="loaiTaiSanID" column="LoaiTaiSanID"/>	
		
		
		<many-to-one name="loaiTaiSan" column="LoaiTaiSanID" class="DMLoaiTaiSan" insert="false" update="false" />				
	</class>

</hibernate-mapping> 