<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="HDUyQuyen" table="hd_uyquyen" lazy="true">	
		<id name="hopDongUyQuyenID" type="integer">
            <column name="HopDongUyQuyenID" />
            <generator class="foreign">
                <param name="property">hopDong</param>
            </generator>
        </id>
        <one-to-one name="hopDong" class="HDHopDong" constrained="true" />
        
        
		<property name="phamViUyQuyen" column="PhamViUyQuyen"/>
		<property name="thoiHanUyQuyen" column="ThoiHanUyQuyen"/>
		<property name="thoiHanKetThucUyQuyen" column="ThoiHanKetThucUyQuyen"/>
		<property name="uyQuyenTuNgay" column="UyQuyenTuNgay"/>
		<property name="thuLaoPhaiTra" column="ThuLaoPhaiTra"/>
		<property name="ngayCoHieuLuc" column="NgayCoHieuLuc"/>
		<property name="cacThoaThuanKhac" column="CacThoaThuanKhac"/>
	</class>

</hibernate-mapping> 