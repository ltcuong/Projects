<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="HDHopDongDuongSu" table="hopdong_duongsu" lazy="true">				
		<id name="hopDongDuongSuID" type="integer" column="HopDongDuongSuID">
			<generator class="increment"></generator>
		</id>	
		<property name="hopDongID" column="HopDongID"/>
		<property name="duongSuID" column="DuongSuID"/>
		<property name="loaiDuongSuID" column="LoaiDuongSuID"/>
		
		
		<property name="laDongSoHuu" column="LaDongSoHuu"/>	
		<property name="laNguoiUyQuyen" column="LaNguoiUyQuyen"/>
		<property name="LaDuongSuChinh" column="laDuongSuChinh"/>
			
		<many-to-one name="hopDong" column="HopDongID" class="HDHopDong" insert="false" update="false" />	
		<many-to-one name="duongSu" column="DuongSuID" class="DSDuongSu" insert="false" update="false" />							
	</class>
</hibernate-mapping> 