<hibernate-mapping package="entity">
	<class name="TBLNhatKySaoLuu" table="tbl_nhatkysaoluu" lazy="true">	
		<id name="saoLuuID" type="integer" column="SaoLuuID">	
		<generator class="increment"></generator>	
		</id>	
		<property name="tenSaoLuu" column="TenSaoLuu"/>	
		<property name="ngaySaoLuu" column="NgaySaoLuu"/>	
		<property name="taiKhoanSaoLuu" column="TaiKhoanSaoLuu"/>	
		<property name="duongDan" column="DuongDan"/>	
		
		
		<!-- <many-to-one name="userGroup" column="GroupID" class="SystemGroup" insert="false" update="false" /> -->				
	</class>
</hibernate-mapping> 