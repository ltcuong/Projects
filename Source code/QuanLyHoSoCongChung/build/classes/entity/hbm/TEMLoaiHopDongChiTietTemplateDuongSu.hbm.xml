<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="TEMLoaiHopDongChiTietTemplateDuongSu" table="tem_loaihopdongchitet_templateduongsu" lazy="true">				
		<composite-id> 	
			<key-property name="loaiHopDongChiTietID" column="LoaiHopDongChiTietID" />				
			<key-property name="templateDuongSuID" column="TemplateDuongSuID" />	
		</composite-id> 	
			
		<many-to-one name="templateDuongSu" column="TemplateDuongSuID" class="TEMTemplateDuongSu" insert="false" update="false" />	
		<many-to-one name="loaiHopDongChiTiet" column="LoaiHopDongChiTietID" class="DMLoaiHopDongChiTiet" insert="false" update="false" />							
	</class>
</hibernate-mapping> 