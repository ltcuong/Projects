<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="TBLLogDetail" table="tbl_logdetails" lazy="true">	
		<id name="logDetailID" type="integer" column="LogDetailID">
			<generator class="increment"></generator>
		</id>	
		<property name="logID" column="LogID"/>	
		<property name="logName" column="LogName"/>	
		<property name="noiDungCu" column="NoiDungCu"/>	
		<property name="noiDungMoi" column="NoiDungMoi"/>	
		
		<many-to-one name="logHeThong" column="LogID" class="TBLLogHeThong" insert="false" update="false" />				
	</class>
</hibernate-mapping> 