<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="TSMayBay" table="ts_maybay" lazy="true">	
		<id name="taiSanMayBayID" type="integer">
            <column name="TaiSanMayBayID" />
            <generator class="foreign">
                <param name="property">taiSan</param>
            </generator>
        </id>
        
		<property name="tenPhuongTien" column="TenPhuongTien"/>	
		<property name="dacDiemTaiSan" column="DacDiemTaiSan"/>	
		<property name="ngayCapGiayDangKy" column="NgayCapGiayDangKy"/>
		<property name="noiCapGiayDangKy" column="NoiCapGiayDangKy"/>		
							
		<one-to-one name="taiSan" class="TSTaiSan" constrained="true" />						
	</class>
</hibernate-mapping> 