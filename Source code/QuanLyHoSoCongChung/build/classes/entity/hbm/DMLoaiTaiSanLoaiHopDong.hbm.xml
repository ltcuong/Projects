<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="DMLoaiTaiSanLoaiHopDong" table="dm_loaitaisan_hopdong" lazy="true">				
		<composite-id> 	
			<key-property name="loaiTaiSanID" column="LoaiTaiSanID" />				
			<key-property name="loaiHopDongChiTietID" column="LoaiHopDongChiTietID" />	
		</composite-id> 	
			
		<many-to-one name="loaiTaiSan" column="LoaiTaiSanID" class="DMLoaiTaiSan" insert="false" update="false" />	
		<many-to-one name="loaiHopDongChiTiet" column="LoaiHopDongChiTietID" class="DMLoaiHopDongChiTiet" insert="false" update="false" />							
	</class>
</hibernate-mapping> 