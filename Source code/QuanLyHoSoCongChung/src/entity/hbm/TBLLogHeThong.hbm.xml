<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="TBLLogHeThong" table="tbl_logs" lazy="true">	
		<id name="logID" type="integer" column="LogID">
			<generator class="increment"></generator>
		</id>	
		<property name="tenDangNhap" column="TenDangNhap"/>	
		<property name="actionName" column="ActionName"/>
		<property name="actionContent" column="ActionContent"/>
		<property name="actionTime" column="ActionTime"/>
		<property name="formLamViec" column="FormLamViec"/>
		<property name="formLamViecTen" column="FormLamViecTen"/>
		
		<!-- <many-to-one name="huyen" column="huyen_maso" class="Huyen" insert="false" update="false" /> -->				
	</class>
</hibernate-mapping> 