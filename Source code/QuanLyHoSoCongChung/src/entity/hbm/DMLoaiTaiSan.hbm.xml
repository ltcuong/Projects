<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="DMLoaiTaiSan" table="dm_loaitaisan" lazy="true">	
		<id name="loaiTaiSanID" type="integer" column="LoaiTaiSanID">
			<generator class="increment"></generator>
		</id>			
		<property name="loaiTaiSanTen" column="LoaiTaiSanTen"/>
		<property name="coGiaoDichMotPhan" column="CoGiaoDichMotPhan"/>	
	</class>

</hibernate-mapping> 