<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="DMQuocTich" table="dm_quoctich" lazy="true">	
		<id name="quocTichID" type="integer" column="QuocTichID">
			<generator class="increment"></generator>
		</id>	
		<property name="quocTichMa" column="QuocTichMa"/>	
		<property name="quocTichTen" column="QuocTichTen"/>	
		<property name="ghiChu" column="GhiChu"/>		
		<property name="locked" column="Locked"/>		
	
		<!-- <many-to-one name="huyen" column="huyen_maso" class="Huyen" insert="false" update="false" /> -->				
	</class>
</hibernate-mapping> 