<hibernate-mapping package="entity">
	<class name="DMLoaiDuongSu" table="dm_loaiduongsu" lazy="true">	
		<id name="loaiDuongSuID" type="integer" column="LoaiDuongSuID">
			<generator class="increment"></generator>
