<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">
	<class name="TEMTuKhoaLoaiTaiSan" table="tem_tukhoaloaitaisan" lazy="true">	
		<id name="tuKhoaLoaiTaiSanID" column="TuKhoaLoaiTaiSanID"/>
			
		<property name="tenTuKhoaLoaiTaiSan" column="TenTuKhoaLoaiTaiSan"/>
		<property name="tenHienThiTuKhoaLoaiTaiSan" column="TenHienThiTuKhoaLoaiTaiSan"/>		
		<property name="loaiTaiSanID" column="LoaiTaiSanID"/>	
		
		
		<many-to-one name="loaiTaiSan" column="LoaiTaiSanID" class="DMLoaiTaiSan" insert="false" update="false" />				
	</class>

</hibernate-mapping> 