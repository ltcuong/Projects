<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="DMLoaiHopDongChiTiet" table="dm_loaihopdongchitiet" lazy="true">	
		<id name="loaiHopDongChiTietID" type="integer" column="LoaiHopDongChiTietID">
			<generator class="increment"></generator>
		</id>			
		<property name="loaiHopDongChiTietTen" column="LoaiHopDongChiTietTen"/>	
		<property name="loaiHopDongID" column="LoaiHopDongID"/>
		<property name="coTaiSan" column="CoTaiSan"/>
		<property name="coBenThu3" column="CoBenThu3"/>
		<property name="coDoiChuSoHuu" column="CoDoiChuSoHuu"/>
		<property name="coTaiSanTuongLai" column="CoTaiSanTuongLai"/>
		<property name="coTaiSanGanLien" column="CoTaiSanGanLien"/>
		<property name="noiDungTemplate" column="NoiDungTemplate"/>
		
		<many-to-one name="loaiHopDong" column="LoaiHopDongID" class="DMLoaiHopDong" insert="false" update="false" />
		
	</class>

</hibernate-mapping> 