<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="TSMayMoc" table="ts_maymoc" lazy="true">	
		<id name="taiSanMayMocID" type="integer">
            <column name="TaiSanMayMocID" />
            <generator class="foreign">
                <param name="property">taiSan</param>
            </generator>
        </id>
        
		<property name="nhanHieu" column="NhanHieu"/>	
		<property name="congSuat" column="CongSuat"/>			
		<property name="noiCheTao" column="NoiCheTao"/>
		<property name="dacDiemTaiSan" column="DacDiemTaiSan"/>
		<property name="coQuanDangKiem" column="CoQuanDangKiem"/>		
		
		<one-to-one name="taiSan" class="TSTaiSan" constrained="true" />													
	</class>
</hibernate-mapping> 