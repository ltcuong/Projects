<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="HDFileDinhKemHopDong" table="hd_filedinhkemhopdong" lazy="true">	
		<id name="fileDinhKemID" type="integer" column="FileDinhKemID">
			<generator class="increment"></generator>
		</id>			
		<property name="tenFile" column="TenFile"/>	
		<property name="duongDan" column="DuongDan"/>	
		<property name="hopDongID" column="HopDongID"/>	
		
		<many-to-one name="hopDong" column="HopDongID" class="HDHopDong" insert="false" update="false" />
	</class>

</hibernate-mapping> 