<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="DMTinhTrang" table="dm_tinhtrang" lazy="true">	
		<id name="tinhTrangID" type="integer" column="TinhTrangID">
			<generator class="increment"></generator>
		</id>			
		<property name="tinhTrangTen" column="TinhTrangTen"/>	
		<property name="coTheChinhSua" column="CoTheChinhSua"/>	
		<property name="coTheXoa" column="CoTheXoa"/>	
		
	</class>

</hibernate-mapping> 